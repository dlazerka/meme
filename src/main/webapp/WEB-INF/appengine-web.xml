<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>lazerka-meme</application>
	<version>03-images-stored</version>

	<threadsafe>true</threadsafe>

    <static-files>
	    <include path="/favicon.ico"/>
	    <include path="/robots.txt"/>

		<include path="/font/*" expiration="365d"/>

		<include path="/image/**.png"/>
		<include path="/image/**.jpeg"/>
		<include path="/image/**.jpg"/>
		<include path="/image/**.svg"/>

		<include path="/**.js"/>
		<include path="/**.css"/>
		<include path="/**.html"/>
	</static-files>

	<!--
	<resource-files>
		<include path="/**.css" />
		<include path="/**.js" />
		<include path="/index.html" />
	</resource-files>
	-->

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
</appengine-web-app>
